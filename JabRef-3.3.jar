<script>


    $(function () {
        $('.xlink').click(function () {
            var elem = $(this);
            $.ajax({
                type: "GET",
                url: "/updateStats/" + elem.attr('file'),
                success: function (data) {
                    if (data == 'alldone') {
                        window.location.href = elem.attr('data');
                    }
                }
            });
            return false;
        });
    });


    $(document).ready(function () {

        $("#submitForm").click(function () {
            $.ajax({
                type: "POST",
                url: "/JabRef.html", //process to mail
                data: $('form#reviewForm').serialize(),
                success: function (msg) {
                    $("#your-review").modal('hide'); //hide popup
                    $("#done-review").modal('show'); //hide popup
                    $('html, body').animate({scrollTop : 0},800);
                },
                error: function () {
                    alert("failure");
                }
            });
        });
        $(".showInfoModal").click(function () {
            $("#showSignatures").modal('show'); //hide popup
        });
    });
</script>


